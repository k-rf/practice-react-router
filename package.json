{
  "name": "practice-react-router",
  "version": "1.0.0",
  "description": "The practice of React Router.",
  "author": "Kawano Kenichi <kenichi.k.820@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix --cache --ext .tsx,.ts,.js ./src",
    "format": "prettier . --write",
    "check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "dotenv-cli": "^4.1.1",
    "esbuild-register": "^3.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "msw": "^0.36.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "storybook": "^6.4.14",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.13",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
